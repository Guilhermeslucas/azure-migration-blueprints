{
    "properties": {
        "description": "This is my first attempt of creating a blueprint as code. Lets see how it goes!",
        "targetScope": "subscription",
        "parameters": {
            "vmSizeGen": {
                "type": "string",
                "metadata": {
                    "displayName": "Type of VM"
                },
                "defaultValue": "Standard_A1"
            },
            "locationGen": {
                "type": "string",
                "metadata": {
                    "displayName": "Location to deploy everything"
                },
                "defaultValue": "westus"
            },
            "adminUsernameGen": {
                "type": "string",
                "metadata": {
                    "displayName": "User to log in"
                },
                "defaultValue": "gulucasd"
            },
            "adminPasswordGen": {
                "type": "string",
                "metadata": {
                    "displayName": "Password to login"
                },
                "defaultValue": "awesomepassword123456!@#$%"
            },
            "localNetworkPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Local Network Prefix"
                },
                "defaultValue" : "10.3.0.0/24"
            },
            "localGAtewayIpAddressGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Ip from local gateway"
                },
                "defaultValue" : "1.1.1.1"
            },
            "SharedKeyConnectionGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Shared Key Value for VPN Connection"
                },
                "defaultValue" : "awesomesharedkey12345!@#$%"
            },
            "vnetAddressPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Adress prefix to Vnet"
                },
                "defaultValue" : "10.0.0.0/16"
            },
            "defaultSubnetPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Adress prefix to Vnet"
                },
                "defaultValue" : "10.0.0.0/24"
            },
            "cloudSubnetPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Adress prefix to Vnet"
                },
                "defaultValue" : "10.0.200.0/29"
            },
            "secondaryVnetPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Adress prefix to secoundary Azure Vnet"
                },
                "defaultValue" : "192.168.0.0/24"
            },
            "secondarySubnetPrefixGen": {
                "type": "string",
                "metadata" : {
                    "displayName": "Adress prefix to secondary Azure subnet"
                },
                "defaultValue" : "192.168.0.0/24"
            },
            "ubuntuOSVersionGen": {
                "type": "string",
                "allowedValues": [
                    "12.04.5-LTS",
                    "14.04.5-LTS",
                    "16.04.0-LTS",
                    "18.04-LTS"
                ],
                "metadata": {
                    "description": "The Ubuntu version for the VM."
                },
                "defaultValue": "16.04.0-LTS"
            }
        },
        "resourceGroups": {
            "contoso-rg": {
                "name": "contoso-rg",
                "location": "westus"
            }
        }
    },
    "type": "Microsoft.Blueprint/blueprints"
}