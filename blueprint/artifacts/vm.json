{
    "properties": {
        "template": {
            "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
            "contentVersion": "1.0.0.0",
            "parameters": {
                "vmSize": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "adminUsername": {
                    "type": "string"
                },
                "adminPassword": {
                    "type": "string"
                },
                "windowsOSVersion": {
                    "type": "string"
                },
                "localNetworkPrefix": {
                    "type": "string"
                },
                "localGAtewayIpAddress": {
                    "type": "string"
                },
                "SharedKeyConnection": {
                    "type": "string"
                },
                "vnet1AddressPrefix": {
                    "type": "string"
                },
                "vnet1STSubnetPrefix": {
                    "type": "string"
                },
                "vnet1BackupSubnetPrefix": {
                    "type": "string"
                },
                "vnet1ConnectivitySubnetPrefix": {
                    "type": "string"
                },
                "vnet1DMZSubnetPrefix": {
                    "type": "string"
                },
                "vnet1ManagementSubnetPrefix": {
                    "type": "string"
                },
                "vnet1Reserva1SubnetPrefix": {
                    "type": "string"
                },
                "vnet1Reserva2SubnetPrefix": {
                    "type": "string"
                },
                "vnet2Prefix": {
                    "type": "string"
                },
                "vnet2DBSubnetPrefix": {
                    "type": "string"
                },
                "vnet2ServicesPrefix": {
                    "type": "string"
                },
                "vnet2ManagementPrefix": {
                    "type": "string"
                },
                "vnet2MonitoringPrefix": {
                    "type": "string"
                },
                "vnet2AplicationsSubnetPrefix": {
                    "type": "string"
                },
                "vnet2Reserva1SubnetPrefix": {
                    "type": "string"
                },
                "vnet2Reserva2SubnetPrefix": {
                    "type": "string"
                },
                "vnet2Reserva3SubnetPrefix": {
                    "type": "string"
                }
            },
            "variables": {
                "deployment-prefix": "blueprint-prefix",
                "gatewaySubnetRef": "[resourceId('Microsoft.Network/virtualNetworks/subnets', variables('vnet1Name'), 'GatewaySubnet')]",
                "connectionName": "vpnConnection",
                "localGatewayName": "localGateway",
                "cloudGatewayName": "cloudGateway",
                "nsgName": "blueprintNsg",
                "vnet1Name": "vnet-1",
                "vnet2Name": "vnet-2"
            },
            "resources": [
                {
                    "apiVersion": "2018-07-01",
                    "type": "Microsoft.Network/localNetworkGateways",
                    "name": "[variables('localGatewayName')]",
                    "location": "[parameters('location')]",
                    "properties": {
                        "localNetworkAddressSpace": {
                            "addressPrefixes": [
                                "[parameters('localNetworkPrefix')]"
                            ]
                        },
                        "gatewayIpAddress": "[parameters('localGAtewayIpAddress')]"
                    }
                },
                {
                    "apiVersion": "2018-07-01",
                    "name": "[variables('connectionName')]",
                    "type": "Microsoft.Network/connections",
                    "location": "[parameters('location')]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('cloudGatewayName'))]",
                        "[resourceId('Microsoft.Network/localNetworkGateways', variables('localGatewayName'))]"
                    ],
                    "properties": {
                        "virtualNetworkGateway1": {
                            "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('cloudGatewayName'))]"
                        },
                        "localNetworkGateway2": {
                            "id": "[resourceId('Microsoft.Network/localNetworkGateways', variables('localGatewayName'))]"
                        },
                        "connectionType": "IPsec",
                        "routingWeight": 10,
                        "sharedKey": "[parameters('SharedKeyConnection')]"
                    }
                },
                {
                    "apiVersion": "2018-07-01",
                    "type": "Microsoft.Network/publicIPAddresses",
                    "name": "gateway-pip",
                    "location": "[parameters('location')]",
                    "properties": {
                        "publicIPAllocationMethod": "Dynamic"
                    }
                },
                {
                    "apiVersion": "2018-07-01",
                    "type": "Microsoft.Network/virtualNetworkGateways",
                    "name": "[variables('cloudGatewayName')]",
                    "location": "[parameters('location')]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Network/publicIPAddresses', 'gateway-pip')]",
                        "[resourceId('Microsoft.Network/virtualNetworks', variables('vnet1Name'))]"
                    ],
                    "properties": {
                        "ipConfigurations": [
                            {
                                "properties": {
                                    "privateIPAllocationMethod": "Dynamic",
                                    "subnet": {
                                        "id": "[variables('gatewaySubnetRef')]"
                                    },
                                    "publicIPAddress": {
                                        "id": "[resourceId('Microsoft.Network/publicIPAddresses', 'gateway-pip')]"
                                    }
                                },
                                "name": "vnetGatewayConfig"
                            }
                        ],
                        "sku": {
                            "name": "Basic",
                            "tier": "Basic"
                        },
                        "gatewayType": "Vpn",
                        "vpnType": "RouteBased",
                        "enableBgp": "false"
                    }
                },
                {
                    "type": "Microsoft.Network/networkSecurityGroups",
                    "apiVersion": "2019-08-01",
                    "name": "[variables('nsgName')]",
                    "location": "[parameters('location')]",
                    "properties": {
                        "securityRules": [
                            {
                                "name": "default-allow-22",
                                "properties": {
                                    "priority": 1000,
                                    "access": "Allow",
                                    "direction": "Inbound",
                                    "destinationPortRange": "22",
                                    "protocol": "TCP",
                                    "sourceAddressPrefix": "*",
                                    "sourcePortRange": "*",
                                    "destinationAddressPrefix": "*"
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "Microsoft.Network/virtualNetworks",
                    "apiVersion": "2018-11-01",
                    "name": "[variables('vnet1Name')]",
                    "location": "[parameters('location')]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgName'))]"
                    ],
                    "properties": {
                        "addressSpace": {
                            "addressPrefixes": [
                                "[parameters('vnet1AddressPrefix')]"
                            ]
                        },
                        "subnets": [
                            {
                                "name": "management-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1ManagementSubnetPrefix')]",
                                    "networkSecurityGroup": {
                                        "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgName'))]"
                                    }
                                }
                            },
                            {
                                "name": "GatewaySubnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1ConnectivitySubnetPrefix')]"
                                }
                            },
                            {
                                "name": "servicios-redes-seguridad-transversales-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1STSubnetPrefix')]"
                                }
                            },
                            {
                                "name": "dmz-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1DMZSubnetPrefix')]"
                                }
                            },
                            {
                                "name": "backup-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1BackupSubnetPrefix')]"
                                }
                            },
                            {
                                "name": "reserva1-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1Reserva1SubnetPrefix')]"
                                }
                            },
                            {
                                "name": "reserva2-subnet",
                                "properties": {
                                    "addressPrefix": "[parameters('vnet1Reserva2SubnetPrefix')]"
                                }
                            }
                        ]
                    },
                    "resources": [
                        {
                            "apiVersion": "2016-06-01",
                            "type": "virtualNetworkPeerings",
                            "name": "vnet1to2peering",
                            "location": "[parameters('location')]",
                            "dependsOn": [
                                "[concat('Microsoft.Network/virtualNetworks/', variables('vnet2Name'))]",
                                "[concat('Microsoft.Network/virtualNetworks/', variables('vnet1Name'))]"
                            ],
                            "properties": {
                                "allowVirtualNetworkAccess": "true",
                                "allowForwardedTraffic": "false",
                                "allowGatewayTransit": "false",
                                "useRemoteGateways": "false",
                                "remoteVirtualNetwork": {
                                    "id": "[resourceId('Microsoft.Network/virtualNetworks', variables('vnet2Name'))]"
                                }
                            }
                        }
                    ]
                },
                {
                    "apiVersion": "2016-03-30",
                    "type": "Microsoft.Network/virtualNetworks",
                    "name": "[variables('vnet2Name')]",
                    "location": "[parameters('location')]",
                    "properties": {
